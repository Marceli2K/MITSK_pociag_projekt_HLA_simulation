<?xml version='1.0' encoding='utf-8'?>
<objectModel
        xmlns='http://standards.ieee.org/IEEE1516-2010'
        xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
        xsi:schemaLocation='http://standards.ieee.org/IEEE1516-2010 http://standards.ieee.org/downloads/1516/1516.2-2010/IEEE1516-DIF-2010.xsd'>
    <modelIdentification>
        <name>PociagFederation</name>
        <type>FOM</type>
        <version>1.0</version>
    </modelIdentification>
    <objects>
        <objectClass>
            <name>HLAobjectRoot</name>
            <objectClass>
                <name>Pociag</name>
                <sharing>PublishSubscribe</sharing>
                <semantics>Magazyn dla produktow</semantics>
                <attribute>
                    <name>available</name>
                    <dataType>HLAinteger32BE</dataType>
                    <updateType>Conditional</updateType>
                    <updateCondition>On Change</updateCondition>
                    <ownership>Divest</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <transportation>HLAreliable</transportation>
                    <order>Receive</order>
                    <semantics>Liczba osiagalnych produktow w magazynie</semantics>
                </attribute>
                <attribute>
                    <name>max</name>
                    <dataType>HLAinteger32BE</dataType>
                    <updateType>Static</updateType>
                    <updateCondition>On Change</updateCondition>
                    <ownership>Divest</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <transportation>HLAreliable</transportation>
                    <order>Receive</order>
                    <semantics>N/A</semantics>
                </attribute>
            </objectClass>
        </objectClass>
    </objects>
    <interactions>
        <interactionClass>
            <name>HLAinteractionRoot</name>
            <interactionClass>
                <name>PasazerManagment</name>
                <sharing>PublishSubscribe</sharing>
                <transportation>HLAreliable</transportation>
                <order>Receive</order>
                <semantics>Zdarzenia zarzadzania liczba pasazerow</semantics>
                <parameter>
                    <name>count</name>
                    <dataType>HLAinteger32BE</dataType>
                    <semantics>Liczba pasazerow ktora wsiadla</semantics>
                </parameter>
                <!--                <interactionClass>-->
                <!--                    <name>AddPasazer</name>-->
                <!--                    <sharing>PublishSubscribe</sharing>-->
                <!--                    <transportation>HLAreliable</transportation>-->
                <!--                    <order>Receive</order>-->
                <!--                    <semantics>Zdarzenie dodania nowych pasazerow</semantics>-->
                <!--                </interactionClass>-->
                <!--                <interactionClass>-->
                <!--                    <name>GetProducts</name>-->
                <!--                    <sharing>Publish</sharing>-->
                <!--                    <transportation>HLAbestEffort</transportation>-->
                <!--                    <order>Receive</order>-->
                <!--                    <semantics>Zdarzenia pobrania produktow</semantics>-->
                <!--                </interactionClass>-->
                <interactionClass>
                    <name>SzukajMiejsca</name>
                    <sharing>Publish</sharing>
                    <transportation>HLAbestEffort</transportation>
                    <order>Receive</order>
                    <semantics>Zdarzenia poszukiwania miejsca</semantics>
                </interactionClass>
                <interactionClass>
                    <name>NewPasazer</name>
                    <sharing>PublishSubscribe</sharing>
                    <transportation>HLAreliable</transportation>
                    <order>TimeStamp</order>
                    <parameter>
                        <name>countNewPasazer</name>
                        <dataType>HLAinteger32BE</dataType>
                        <semantics>Client identification number</semantics>
                    </parameter>
                </interactionClass>
            </interactionClass>
        </interactionClass>
    </interactions>
    <dimensions/>
    <synchronizations>
        <synchronizationPoint>
            <label>ReadyRoRun</label>
            <dataType>NA</dataType>
            <capability>NA</capability>
            <semantics>Waiting for each federate joining</semantics>
        </synchronizationPoint>
    </synchronizations>
